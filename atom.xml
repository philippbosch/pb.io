---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>Philipp Bosch. Web &amp; Mobile Development</title>
    <link href="http://pb.io/atom.xml" rel="self"/>
    <link href="http://pb.io/"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://pb.io/</id>
    <author>
        <name>Philipp Bosch</name>
        <email>hello@pb.io</email>
    </author>
    {% for post in site.posts %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="http://pb.io{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://pb.io{{ post.id }}</id>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}
</feed>
